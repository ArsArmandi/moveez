{
  "name": "moveez",
  "version": "1.0.0",
  "description": "a webbased service to keep track of the movies you would like to watch",
  "main": "app.js",
  "scripts": {
    "babel": "babel ./src --out-dir ./dist --copy-files",
    "webpack": "webpack --config webpack.config.js",
    "test": "PORT='8081' RELEASE='mocha test' nyc --reporter=lcov --reporter=text mocha 'test/integration/*.js' --timeout 20000 --full-trace",
    "start": "cd app && NODE_CONFIG_DIR='../config' node app.js",
    "watch": "nodemon --watch ./src --exec npm run dev",
    "dev": "npm run babel && npm run webpack && cd ./dist/app && PORT='8081' RELEASE='local dev build' NODE_CONFIG_DIR='../../config' node ./app.js",
    "cy:open": "cypress open",
    "cy:verify": "cypress verify",
    "uat": "cypress run"
  },

  "repository": {
    "type": "git",
    "url": "git+https://github.com/schdief/moveez.git"
  },
  "keywords": [
    "moveez"
  ],
  "author": "schdief",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/schdief/moveez/issues"
  },
  "homepage": "http://www.moveez.de",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "body-parser": "^1.18.2",
    "config": "^1.29.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "http-status-codes": "^1.3.0",
    "method-override": "^2.3.10",
    "mongoose": "5",
    "morgan": "^1.9.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-transform-modules-systemjs": "^7.1.3",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "chai": "^3.5.0",
    "chai-http": "^2.0.1",
    "cypress": "^3.1.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.18.4",
    "nyc": "^11.4.1",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2"
  }
}
