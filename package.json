{
  "name": "moveez",
  "version": "1.0.0",
  "description": "a webbased service to keep track of the movies you would like to watch",
  "main": "app.js",
  "scripts": {
    "test": "PORT='8081' RELEASE='mocha test' nyc --reporter=lcov --reporter=text mocha 'test/integration/*.js' --timeout 20000 --full-trace",
    "start": "NODE_ENV=prod NODE_CONFIG_DIR='../config' && cd app && node app.js",
    "dev": "cd app && PORT='8081' RELEASE='local dev build' NODE_CONFIG_DIR='../config' nodemon app.js",
    "cy:open": "cypress open",
    "cy:verify": "cypress verify",
    "uat": "cypress run --record --browser chrome"
  },

  "repository": {
    "type": "git",
    "url": "git+https://github.com/schdief/moveez.git"
  },
  "keywords": [
    "moveez"
  ],
  "author": "schdief",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/schdief/moveez/issues"
  },
  "homepage": "http://www.moveez.de",
  "dependencies": {
    "body-parser": "^1.18.2",
    "config": "^1.29.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "http-status-codes": "^1.3.0",
    "method-override": "^2.3.10",
    "mongoose": "5",
    "morgan": "^1.9.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^3.5.0",
    "chai-http": "^2.0.1",
    "cypress": "^3.1.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.18.4",
    "nyc": "^11.4.1"
  }
}
