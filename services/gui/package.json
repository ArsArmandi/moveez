{
  "name": "moveez",
  "version": "1.0.0",
  "description": "manage your binge!",
  "main": "app.js",
  "scripts": {
    "babel": "babel src --out-dir dist --copy-files",
    "test": "PORT='8082' RELEASE='mocha test' nyc --reporter=lcov --reporter=text --reporter=cobertura mocha 'test/integration/*.js' --timeout 20000 --full-trace",
    "start": "cd app && NODE_CONFIG_DIR='../config' node app.js",
    "watch": "nodemon -e js,ejs,css --watch ./src --exec npm run dev",
    "dev": "npm run babel && cd ./dist/app && KETCHUP_ENDPOINT='localhost:8083' PORT='8443' TLS_CRT_PATH='../../tlscert/server.crt' TLS_KEY_PATH='../../tlscert/server.key' FACEBOOK_APP_SECRET_PATH='../../facebook/app_secret' NODE_CONFIG_DIR='../../config' node ./app.js",
    "forUat": "cd ./dist/app && KETCHUP_ENDPOINT='localhost:8083' PORT='8443' NODE_CONFIG_DIR='../../config' NODE_ENV='uat' node ./app.js",
    "cy:open": "cypress open",
    "cy:verify": "cypress verify",
    "uat": "cypress run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schdief/moveez.git"
  },
  "keywords": [
    "moveez"
  ],
  "author": "schdief",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/schdief/moveez/issues"
  },
  "homepage": "http://www.moveez.de",
  "dependencies": {
    "ajv": "^6.9.1",
    "body-parser": "^1.18.2",
    "config": "^1.29.0",
    "connect-ensure-login": "^0.1.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.6.5",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "http-status-codes": "^1.3.0",
    "method-override": "^2.3.10",
    "mongoose": "^5.5.3",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-http": "^0.3.0",
    "superagent": "^4.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-transform-modules-systemjs": "^7.1.3",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "chai": "^3.5.0",
    "chai-http": "^2.0.1",
    "file-loader": "^2.0.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.18.4",
    "nyc": "^11.4.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "sinon": "^7.2.7"
  },
  "optionalDependencies": {
    "cypress": "^3.2.0"
  }
}
